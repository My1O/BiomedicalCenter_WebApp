@model IEnumerable<BiomedicalWebApp.Models.Turno>

@{
    ViewData["Title"] = "Turno";
}

<h1>Turno</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaTurno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Observacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstadoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaTurno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstadoNavigation.IdEstado)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdTurno">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdTurno">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdTurno">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
